{
  "extends": ["eslint:recommended", "plugin:jest/recommended"],
  "env": {
      "es6": true,
      "node": true,
      "jest": true
  },
  "parserOptions": {
      "ecmaVersion": 9
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "arrow-parens": ["error", "always"],
    "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
  }
}
